<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GlassButton">
        <attr name="android:text" />
        <attr name="tint" format="color" />
        <attr name="surfaceColor" format="color" />
        <attr name="interactive" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GlassIconButton">
        <attr name="android:src" />
    </declare-styleable>

    <declare-styleable name="GlassSlider">
        <attr name="android:orientation" />
        <attr name="value" format="float" />
        <attr name="valueFrom" format="float" />
        <attr name="valueTo" format="float" />
    </declare-styleable>
</resources>
