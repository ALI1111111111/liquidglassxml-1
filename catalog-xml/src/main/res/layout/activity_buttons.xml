<?xml version="1.0" encoding="utf-8"?>
<com.kyant.backdrop.catalog.xml.components.LayerBackdropView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/backdropView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@drawable/system_home_screen_light"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp"
        android:layout_marginTop="30dp"
        android:gravity="center_horizontal">




        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:id="@+id/liquidGlassView"
            android:layout_width="300dp"
            android:layout_height="150dp"
            android:layout_gravity="center"
            app:blurRadius="24dp"
            app:cornerRadius="40dp"
            app:glassPreset="none"
            app:highlightAlpha="0.4"
            app:highlightAngle="2.5"
            app:highlightFalloff="0.4"
            app:refractionAmount="80dp"
            app:refractionHeight="40dp"
            app:refractionDepthEffect="true"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="iosButton"
            app:cornerRadius="16dp"
            app:blurRadius="20dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="materialCard"
            app:cornerRadius="24dp"
            app:blurRadius="24dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="frostedOverlay"
            app:cornerRadius="20dp"
            app:blurRadius="18dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="chromaticGlass"
            app:cornerRadius="24dp"
            app:blurRadius="20dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="subtlePanel"
            app:cornerRadius="16dp"
            app:blurRadius="16dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="gamingGlass"
            app:cornerRadius="20dp"
            app:blurRadius="22dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="minimalist"
            app:cornerRadius="12dp"
            app:blurRadius="16dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="heavyGlass"
            app:cornerRadius="28dp"
            app:blurRadius="28dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="notificationPanel"
            app:cornerRadius="20dp"
            app:blurRadius="20dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="pressedButton"
            app:cornerRadius="16dp"
            app:blurRadius="18dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="navigationBar"
            app:cornerRadius="0dp"
            app:blurRadius="20dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="100dp"
            android:layout_height="100dp"
            app:glassPreset="fab"
            app:cornerRadius="28dp"
            app:blurRadius="16dp"
            android:layout_marginBottom="16dp"/>


        <com.kyant.backdrop.xml.views.LiquidGlassView
            android:layout_width="300dp"
            android:layout_height="150dp"
            app:glassPreset="dialogBackdrop"
            app:cornerRadius="24dp"
            app:blurRadius="22dp"
            android:layout_marginBottom="16dp"/>

    </LinearLayout>

</ScrollView>
</com.kyant.backdrop.catalog.xml.components.LayerBackdropView>
